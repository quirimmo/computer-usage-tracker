{
	"name": "computer-usage-tracker",
	"version": "0.0.0",
	"main": "",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "npm run test:electron && npm run test:app",
		"test:watch": "npm run test:electron:watch && npm run test:app:watch",
		"test:electron": "jest --config ./jest.electron.config.js --colors --coverage",
		"test:electron:watch": "jest --config ./jest.electron.config.js --colors --watchAll",
		"test:app": "jest --config ./jest.app.config.js --colors --coverage",
		"test:app:watch": "jest --config ./jest.app.config.js --colors --watchAll",
		"lint": "ng lint",
		"electron": "tsc --p ./tsconfig-electron.json && electron ./dist-electron/main.js --dev",
		"electron:watch": "node ./node_modules/nodemon/bin/nodemon.js",
		"electron:build": "ng build --prod && electron ./dist-electron/main.js"
	},
	"private": true,
	"dependencies": {
		"@angular-redux/form": "^9.0.1",
		"@angular-redux/router": "^9.0.0",
		"@angular-redux/store": "^9.0.0",
		"@angular/animations": "^6.1.10",
		"@angular/cdk": "^7.0.2",
		"@angular/common": "^6.0.3",
		"@angular/compiler": "^6.0.3",
		"@angular/core": "^6.0.3",
		"@angular/forms": "^6.0.3",
		"@angular/http": "^6.0.3",
		"@angular/material": "^7.0.2",
		"@angular/platform-browser": "^6.0.3",
		"@angular/platform-browser-dynamic": "^6.0.3",
		"@angular/router": "^6.0.3",
		"bootstrap": "^4.1.3",
		"core-js": "^2.5.4",
		"font-awesome": "^4.7.0",
		"material-icons": "^0.2.3",
		"nedb": "^1.8.0",
		"ngx-electron": "^1.0.4",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0",
		"rxjs": "^6.0.0",
		"zone.js": "^0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.6",
		"@angular/cli": "~6.0.7",
		"@angular/compiler-cli": "^6.0.3",
		"@angular/language-service": "^6.0.3",
		"@types/nedb": "^1.8.6",
		"@types/node": "~8.9.4",
		"codelyzer": "~4.2.1",
		"cross-env": "^5.2.0",
		"devtron": "^1.4.0",
		"electron": "^3.0.5",
		"jest": "^23.6.0",
		"jest-preset-angular": "^6.0.1",
		"nodemon": "^1.18.4",
		"prettier": "^1.14.3",
		"tslint": "~5.9.1"
	}
